{# Main #}
{% block main %}
    <div role="main" id="content" class="main-container {{ container }} js-quickedit-main-content {{ colorbody }}">
        <!-- Start full top region -->
        {% if page.fullheader %}
            {% block fullheader %}
                <div class="container-fluid s3-m-0 full-top-region">
                    <div class="row">
                        {{ page.fullheader }}
                    </div>
                </div>
            {% endblock %}
        {% endif %}
        <!-- end full top region -->
        <div class="container">
            <!-- removing col-xl-12 <div class="row col-xl-12"><!-- was s3-m-10 -->
            <div class="row"><!-- was s3-m-10 -->
                <div class="s3-w s3-scored">
                    {# Header #}
                    {% if page.header %}
                        {% block header %}
                            <div class="col-sm-12" role="heading">
                                {{ page.header }}
                            </div>
                        {% endblock %}
                    {% endif %}
                    {# Slideshow #}
                    {% if page.slideshow %}
                        {% block slideshow %}
                            <div class="col-sm-12" role="slideshow">
                                {{ page.slideshow }}
                            </div>
                        {% endblock %}
                    {% endif %}
                    {# Sidebar First #}
                    {% if firstsidebar_rendered is not empty %}
                        {% if page.sidebar_first %}
                            {% block sidebar_first %}
                                <aside class="col-sm-2" role="complementary">
                                    {{ page.sidebar_first }}
                                </aside>
                            {% endblock %}
                        {% endif %}
                    {% endif %}


                    {# Content #}
                    {% set content_classes = [
                    firstsidebar_rendered is not empty and secondsidebar_rendered is not empty? 'col-sm-8',
                    firstsidebar_rendered is not empty and secondsidebar_rendered is empty? 'col-sm-10',
                    firstsidebar_rendered is empty and secondsidebar_rendered is not empty ? 'col-sm-10',
                    firstsidebar_rendered is empty and secondsidebar_rendered is empty ? 'col-sm-12'
                    ] %}
                    <section{{ content_attributes.addClass(content_classes) }}>

                        {# Highlighted #}
                        {% if page.highlighted %}
                            {% block highlighted %}
                                <div class="highlighted">{{ page.highlighted }}</div>
                            {% endblock %}
                        {% endif %}

                        {# Breadcrumbs #}
                        {% if breadcrumb %}
                            {% block breadcrumb %}
                                {{ breadcrumb }}
                            {% endblock %}
                        {% endif %}

                        {# Action Links #}
                        {% if action_links %}
                            {% block action_links %}
                                <ul class="action-links">{{ action_links }}</ul>
                            {% endblock %}
                        {% endif %}

                        {# Help #}
                        {% if page.help %}
                            {% block help %}
                                {{ page.help }}
                            {% endblock %}
                        {% endif %}

                        {# Content #}
                        {% block content %}
                            <a id="main-content"></a>
                            {{ page.content }}
                        {% endblock %}
                    </section>

                    {# Sidebar Second #}
                    {% if secondsidebar_rendered is not empty %}
                        {% if page.sidebar_second %}
                            {% block sidebar_second %}
                                <aside class="col-sm-2" role="complementary">
                                    {{ page.sidebar_second }}
                                </aside>
                            {% endblock %}
                        {% endif %}
                    {% endif %}

                </div>
            </div>
        </div>
        <!-- Start full bottom region -->
        {% if page.fullcontent %}
            {% block fullcontent %}
                <div class="s3 {{ colorfullwidth }} s3-scored full-bottom-region">
                    <div class="container">
                        <div class="row">
                            <div class="col-sm-12">
                                {{ page.fullcontent }}
                            </div>
                        </div>
                    </div><!-- .container -->
                </div>
            {% endblock %}
        {% endif %}
        <!-- end full bottom region -->
    </div>
{% endblock %}
<!-- Start advanced content region test -->


{% if page.advancedcontent %}
    {% block advancedcontent %}
        <div class="full-advanced-region">
            {{ page.advancedcontent }}
        </div>
    {% endblock %}
{% endif %}
<!-- end full bottom region -->


{% block footer %}
    <footer id="fsu-footer" class="small footer {{ container }} {{ colortwo }}" role="contentinfo">
        {% if page.footer %}
            <div class="container">
                <div class="row s3-m-0">
                    {{ page.footer }}
                </div>
            </div>
        {% endif %}
        <div class="{{ colortwo }}">
            <div class="container">
                <div class="row s3-m-0">
                    <!-- new variable area TODO -->
                    {# if there is anything to put into the footer from the theme settings place it in here #}
                    {% if footertextone is empty or
                    footertexttwo is empty or
                    maptext is empty and
                    googlemap is not empty and twitterfeed is empty %}
                        {% set emptytext = '1' %}
                    {% endif %}
                    {% if twitterfeed is not empty or
                    locationname is not empty or
                    locationaddress is not empty or
                    locationcity is not empty or
                    locationstate is not empty or
                    locationpostalcode is not empty or
                    locationlat is not empty or
                    locationlon is not empty or
                    footertextone is not empty or
                    footertexttwo is not empty or
                    maptext is not empty or
                    twitterfeed is not empty or
                    instagram is not empty or
                    twitter is not empty or
                    facebook is not empty or
                    youtube is not empty or
                    linkedin is not empty or
                    googlemap is not empty or
                    maplocation is not empty %}

                        <div class="col-xs-12 footer-details">
                            {# Theme area for small text areas and small map #}
                            <div class="row">
                                <div class="col-xs-12 col-md-12 footer-small-area">
                                    {# if map, and social, three text areas #}
                                    {% if maplocation is not empty and twitter is not empty and footertextone is not empty and footertexttwo is not empty and maptext is not empty %}
                                        <div class="col-xs-3 col-md-3"><!-- maptext-footertextone-footertexttwo -->
                                            {{ footertextone|raw }}
                                        </div>
                                        <div class="col-xs-3 col-md-3">
                                            {{ footertexttwo|raw }}
                                        </div>
                                        <div class="col-xs-6 col-md-6">
                                            {{ maptext|raw }}
                                        </div>
                                    {% endif %}
                                    {# / map plus social and three text #}
                                    {# if map, no social, three text areas #}
                                    {% if maplocation is not empty and twitter is empty and footertextone is not empty and footertexttwo is not empty and maptext is not empty %}
                                        <div class="col-xs-3 col-md-3">
                                            <!-- smallmap-footertextone-footertexttwo-maptext -->
                                            {{ footertextone|raw }}
                                        </div>
                                        <div class="col-xs-3 col-md-3">
                                            {{ footertexttwo|raw }}
                                        </div>
                                        <div class="col-xs-3 col-md-3">
                                            {{ googlemapsmall|raw }}
                                        </div>
                                        <div class="col-xs-3 col-md-3">
                                            {{ maptext|raw }}
                                            {% set mapused = '1' %}
                                        </div>
                                    {% endif %}
                                    {# / map plus three text #}

                                    {# if map, no social, two text areas #}
                                    {% if maplocation is not empty and twitter is empty and footertextone is not empty and footertexttwo is not empty and maptext is empty %}
                                        <div class="col-xs-4 col-md-4">
                                            <!-- smallmap-footertextone-footertexttwo-maptext-nosocial -->
                                            {{ footertextone|raw }}
                                        </div>
                                        <div class="col-xs-4 col-md-4">
                                            {{ footertexttwo|raw }} {{ maptext|raw }}
                                        </div>
                                        <div class="col-xs-4 col-md-4">
                                            {{ googlemapsmall|raw }}
                                            {% set mapused = '1' %}
                                        </div>
                                    {% endif %}
                                    {# / map plus two text #}
                                    {# if map, no social, no text area #}
                                    {#% if emptytext is not empty %#}
                                    {% if maplocation is not empty and twitter is empty and footertextone is empty and footertexttwo is empty and maptext is empty %}
                                        <div class="col-xs-6 col-md-6"> <!-- map and not text -->
                                        </div>
                                        <div class="col-xs-6 col-md-6">
                                            {# {{ googlemapsmall|raw }}
                                            {% set mapused = '1' %} #}
                                        </div>
                                    {% endif %}
                                    {# if map, no social, one text area #}
                                    {% if maplocation is not empty and twitter is empty and footertextone is not empty and footertexttwo is empty and maptext is empty %}
                                        <div class="col-xs-6 col-md-6"><!-- map and footer text one -->
                                            {{ footertextone|raw }}
                                        </div>
                                        <div class="col-xs-6 col-md-6">
                                            {{ googlemapsmall|raw }}
                                            {% set mapused = '1' %}
                                        </div>
                                    {% endif %}
                                    {# / map plus one text #}

                                    {# no map, three items #}
                                    {% if maplocation is empty and footertextone is not empty and footertexttwo is not empty and maptext is not empty %}
                                        <div class="col-xs-4 col-md-4"><!-- footertextone-footertexttwo-maptext -->
                                            {{ footertextone|raw }}
                                        </div>
                                        <div class="col-xs-4 col-md-4">
                                            {{ footertexttwo|raw }}
                                        </div>
                                        <div class="col-xs-4 col-md-4">
                                            {{ maptext|raw }}
                                        </div>
                                    {% endif %}
                                    {# /no map, three items #}

                                    {# no map, two items #}
                                    {% if maplocation is empty and footertextone is not empty and footertexttwo is not empty and maptext is empty %}
                                        <div class="col-xs-6 col-md-6"><!-- footertextone-footertexttwo -->
                                            {{ footertextone|raw }}
                                        </div>
                                        <div class="col-xs-6 col-md-6">
                                            {{ footertexttwo|raw }}
                                        </div>
                                    {% endif %}
                                    {# /no map, two items #}

                                    {# no map, one items #}
                                    {% if maplocation is empty and footertextone is not empty and footertexttwo is empty and maptext is empty %}
                                        <div class="col-xs-12 col-md-12"><!-- footertextone -->
                                            {{ footertextone|raw }}
                                        </div>

                                    {% endif %}
                                    {# /no map, one items #}

                                </div>
                            </div>
                            {# /small text area #}
                            {# Large footer area for theme map and social if both exist #}
                            {# if map and / or  social are filled out #}
                            {% if maplocation is not empty and twitterfeed is not empty %}
                                <div class="row">
                                    {#
                      <div class="col-xs-12 col-md-6 footer-map-large"><!-- social and map are used -->
                       {% if maplocation is not empty and googlemap is not empty and mapused is empty %}
                         {{ googlemap|raw }}
                       {% endif %}
                      </div>
                                #}

                                    {# added august 21, possibly temp fix #}
                                    {% if maplocation is not empty and twitter is empty and footertextone is empty and footertexttwo is empty and maptext is empty %}
                                        <div class="col-md-6 footer-map-large">
                                            {{ googlemap|raw }}
                                            {% set mapused = '1' %}
                                        </div>

                                    {% endif %}
                                    {# end of aug 21 fix #}


                                    <div class="col-xs-12 col-md-6 social-stream facebook">
                                        {% if twitterfeed is not empty %}
                                            {{ twitterfeed|raw }}
                                        {% endif %}
                                    </div>
                                </div>
                            {% endif %}

                            {# if map and not social are filled out #}
                            {% if maplocation is empty and twitterfeed is not empty %}
                                <div class="row">
                                    <div class="col-xs-12 col-md-12 social-stream"><!-- no map-social only -->
                                        {% if twitterfeed is not empty %}
                                            {{ twitterfeed|raw }}
                                        {% endif %}
                                    </div>
                                </div>
                            {% endif %}

                            {# if social and not map are filled out #}
                            {% if maplocation is not empty and twitterfeed is empty and footertextone is empty and footertexttwo is empty and maptext is empty and mapused is empty %}
